   </suppress>
   <suppress until="2025-02-09">
      <notes><![CDATA[
      file name: jgroups-aws-3.0.0.Final.jar
      ]]></notes>
      <packageUrl regex="true">^pkg:maven/org\.jgroups\.aws/jgroups\-aws@.*$</packageUrl>
